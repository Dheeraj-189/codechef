#include <stdio.h>
int main() {
    int N;
    
    // Read the number of people in the team
    if (scanf("%d", &N) == 1) {
        
        // Check if N is at least 6 AND no more than 8
        if (N >= 6 && N <= 8) {
            printf("Yes\n");
        } else {
            printf("No\n");
        }
    }
    
    return 0;
}
